  },
  "engines": {
    "node": ">=7.x"
  },
  "dependencies": {
    "autobind-decorator": "^2.1.0",
    "fs-extra": "^5.0.0",
    "klaw-sync": "^3.0.2",
    "lodash": "^4.17.10",
    "rimraf": "^2.6.2",
    "util.ds": "~0.0.38",
    "util.home": "~0.0.24",
    "util.join": "~0.0.19",
    "util.log": "~0.0.26",
    "util.promise": "~0.0.22",
    "util.timestamp": "~0.0.37",
    "util.toolbox": "~0.0.59",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/autobind-decorator": "^2.1.0",
    "@types/fs-extra": "5.0.2",
    "@types/lodash": "^4.14.108",
    "@types/node": "^9.6.6",
    "@types/rimraf": "2.0.2",
    "@types/uuid": "^3.4.3",
    "ava": "^0.25.0",
    "coveralls": "^3.0.0",
    "empty-dir": "^1.0.0",
    "nyc": "^11.7.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3",
